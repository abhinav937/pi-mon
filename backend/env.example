# Pi Monitor Backend Configuration
# Copy this file to .env and modify the values

# API Key for authentication (change this in production!)
PI_MONITOR_API_KEY=your-secure-api-key-here

# Server Configuration
BACKEND_PORT=5001
FRONTEND_PORT=80

# Logging
LOG_LEVEL=INFO
LOG_FILE=pi_monitor.log

# Security
MAX_CONNECTIONS=100
REQUEST_TIMEOUT=30
